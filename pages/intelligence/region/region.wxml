<!--pages/intelligence/region/region.wxml-->
<checkbox-group bindchange="serviceValChange">  
  <view class="service_selection">  
    <label class="checkbox {{anyChecked?'is_checked':''}}" >  
      <checkbox value="-1" checked="{{anyChecked}}" hidden="false" />不限  
    </label>
    <block wx:for="{{style}}" wx:key="NAME">
        <label class="checkbox {{item.checked?'is_checked':''}}">  
          <checkbox value="{{index}}" checked="{{item.checked}}" hidden="false" />{{item.NAME}}  
        </label>    
    </block>  
  </view>  
</checkbox-group>