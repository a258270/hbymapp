/* pages/ analog/ analog.wxss */
page{
  background-color:#f2eada;
}
.head{
  width:100%;
  height:300rpx;
}
.head image{
  width:100%;
  height:100%;
}
.content1{
  width:94%;
  height:200rpx;
  background-color:white;
  margin:3% 3%;
  border-radius: 10rpx;
  font-size:28rpx;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
}
.content1-left{
  display: flex;
  flex-direction: column;
  font-size:30rpx;
  margin:45rpx 30rpx;
}
.content1-right{
  display: flex;
  flex-direction: column;
  align-items:center;
  margin:30rpx 30rpx;
}
.content2{
  width:94%;
  margin:0 3%;
  font-size:30rpx;
  display: flex;
  flex-direction: row;
}
.content2 text{
  display: flex;
  align-items:center;
  justify-content: center;
}
.content3{
  width:94%;
  margin:3% 3%;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  
}
button[type="primary"] {
  background-color: rgb(170,233,251);
  color:#333333;
}
.button{
  width:45%;
  font-size:30rpx;
}
.button1{
  font-size:30rpx;
  background-color:#DFDFB8;
}
.content3 picker{
  width:45%;
}
.content4{
  width:94%;
  margin:3% 3%;
}
.footer{
  width:94%;
  height:700rpx;
  margin:260rpx 3%;
  border-radius: 10rpx;
  border:1rpx solid #999999;  
  font-size:28rpx;
}
.footer1{
  padding:0 20rpx;
}
.footer1 text{
  padding:0 10rpx;
  font-size:30rpx;
}
.footer2{
  padding:20rpx 20rpx 0 20rpx;
}